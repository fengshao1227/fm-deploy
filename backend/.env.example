# 应用配置
NODE_ENV=development
PORT=3000
APP_NAME=FM部署系统

# JWT配置
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# 数据库配置（MySQL）
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=MyStrongPassword123!
DB_NAME=fm_deploy

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# SSH密钥存储路径
SSH_KEYS_PATH=/var/ssh-keys

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log

# CORS配置
CORS_ORIGIN=*

# WebSocket配置
WS_PING_TIMEOUT=30000
WS_PING_INTERVAL=25000
